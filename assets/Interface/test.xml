<?xml version="1.0" encoding="UTF-8"?>
<nifty xmlns="http://nifty-gui.sourceforge.net/nifty-1.3.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://nifty-gui.sourceforge.net/nifty-1.3.xsd http://nifty-gui.sourceforge.net/nifty-1.3.xsd">
    <useStyles filename="nifty-default-styles.xml" />
    <useControls filename="nifty-default-controls.xml" />

    <screen id="start">
        <layer childLayout="center">
            <image filename="Interface/skin.png" imageMode="resize:10,108,10,10,10,108,10,108,10,108,10,10" width="50%" height="50%" align="center" valign="center" childLayout="vertical" backgroundColor="#ffff" padding="10">
                <image filename="Interface/skin2.png" imageMode="resize:10,108,10,10,10,108,10,108,10,108,10,10" height="*" width="*" padding="10">
                </image>
                <panel height="10px" />
                <panel height="20px" childLayout="horizontal">
                    <control id="button_ok" name="button" label="Alright" />
                    <panel width="*" />
                    <control id="button_cancel" name="button" label="Cancel" />
                </panel>
            </image>
        </layer>
    </screen>
</nifty>